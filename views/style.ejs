<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Admin</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <link rel="stylesheet" type="text/css" href="../css/style.css" />
  
  </head>
  <body>
    <div class="container-fluid">
        <div class="row vh-100">
            <div class="col-lg-3 bg-secondary ">
                <div class="row py-2 border-bottom">
                    <h1>Admin</h1>
                </div>
                <ul class="list-unstyled">
                  <li class="my-3"><a href="/admin"><i class="bi bi-house-door"></i> User Informaton </a></li>
                  <!-- <li class="my-4-5"><a href="/styles"><i class="bi bi-newspaper"></i> Styles</a></li> -->
                  <li class="my-4-5"><a href="/profile"><i class="bi bi-person-square"></i> Profile</a></li>
                  <li class="my-4-5"><a href="/experience"><i class="bi bi-columns-gap"></i> Experience</a></li>
                  <li class="my-4-5"><a href="/education"><i class="bi bi-columns-gap"></i> Education</a></li>
                  <li class="my-4-5"><a href="/admin/skills"><i class="bi bi-house-door"></i> Skills</a></li>
                  <li class="my-4-5"><a href="/works"><i class="bi bi-book-half"></i> Works</a></li>
                  <li class="my-4-5"><a href="/contact"><i class="bi bi-envelope"></i> Contact</a></li>
                </ul>
            </div>
            <div class="col-lg-9  ">
                <div class="row text-center">
                    <h2> Style User</h2> 
                      
                    <table class="table">
                      <thead>
                        <tr>
                          <th>ID</th>
                          <th>Name</th>
                          <th>Description</th>
                          <th></th>
                          <th>Icon</th>
                          <th>Title</th>
                          <th>Information</th>
                        
                        </tr>
                      </thead>
                      <tbody>
                        <% for (var i =0 ; i < data.length; i++) {%>
                        <tr>
                          <td><%= data[i].id %></td>
                          <td><%= data[i].name %></td>
                          <td><%= data[i].job %></td>
                          <td><img src="../<%= data[i].image %>" class=" img-fluid" width="200px" height="10px"></td>
                          <td><img src="../<%= data[i].icon %>"  class=" img-fluid" width="130px" height="10px"></td>
                          <td><%= data[i].information %></td>

                          <td><a href="/inuser/<%= data[i].id %>">View
                            /<a href="/userdele/<%= data[i].id %>">Delete</a></td>
                  
                        </tr>
                        <% }%>
                        
                      </tbody>
                    </table>
                
          
                </div>
        </div>
    </div>
    
  </body>
</html>