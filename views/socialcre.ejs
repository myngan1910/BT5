<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <link rel="stylesheet" type="text/css" href="../css/style.css" />
  
  </head>
  <body>
    <div class="container-fluid">
        <div class="row vh-100">
            <div class="col-lg-3 bg-secondary ">
                <div class="row py-2 border-bottom">
                    <h1>AdminLTE</h1>
                </div>
                <ul class="list-unstyled">
                    <li class="my-3"><a href="/admin/userInfo"><i class="bi bi-house-door"></i> User Informaton </a></li>
                    <!-- <li class="my-4-5"><a href="/styles"><i class="bi bi-newspaper"></i> Styles</a></li> -->
                    <li class="my-4-5"><a href="/admin/profile"><i class="bi bi-person-square"></i> Profile</a></li>
                    <li class="my-4-5"><a href="/admin/experience"><i class="bi bi-columns-gap"></i> Experience</a></li>
                    <li class="my-4-5"><a href="/admin/education"><i class="bi bi-columns-gap"></i> Education</a></li>
                    <li class="my-4-5"><a href="/admin/skills"><i class="bi bi-house-door"></i> Skills</a></li>
                    <li class="my-4-5"><a href="/admin/works"><i class="bi bi-book-half"></i> Works</a></li>
                    <li class="my-4-5"><a href="/admin/contact"><i class="bi bi-envelope"></i> Contact</a></li>
                    <li class="my-4-5"><a href="/admin/social"><i class="bi bi-envelope"></i> Social</a></li>
                  </ul>
            </div>
            <div class="col-lg-9  ">
                <div class="row ">
                  <h2>Create Social </h2>
                    <form action="/admin/createsocial" method="post" enctype="multipart/form-data">
                       
                      <div class="form-group">
                          <label for="name">Name:</label>
                          <input type="text" class="form-control" id="name" value="" name="name">
                          
                          <label for="icon">Icon:</label>
                          <img> <input type="file"  name="icon" /><br>
                          <label for="link">Link:</label>
                          <input type="link" class="form-control" id="link" value="" name="link">
                          
                          
                          
                        </div>
                      <button type="submit" class="btn btn-dark">Create</button>
                    </form>
                
          
                </div>
                  </div>
            </div>
        </div>
    </div>
    
  </body>
</html>